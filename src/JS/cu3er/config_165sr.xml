<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings >
	
	<general />
		<auto_play >
          <defaults />
          <tweenIn />
          <tweenOut />
          <tweenOver  />
    </auto_play>

    	<prev_button>
			<defaults round_corners="35,35,35,35" y="40" />
			<tweenIn y="40"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1" y="40" />
			<tweenOut tint="0x000000" y="40" />
		</prev_button>
		
    	<prev_symbol >
			<tweenOver tint="0x000000" y="40"/>			
      <tweenIn y="40"/>
		</prev_symbol>

		
    	<next_button >
			<defaults round_corners="35,35,35,35" y="40"/>			
      <tweenIn y="40"/>
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1" y="40"/>
			<tweenOut tint="0x000000" y="40"/>
		</next_button>
		
    	<next_symbol >
    	<tweenIn y="40"/>
			<tweenOver tint="0x000000" y="40" />
		</next_symbol>	
 
			
	</settings>    

	<slides >
	
        <slide width="1200">
            <url>http://localhost/quickstart/public/images/gavrilovic/1.jpg</url>

        </slide>
		<!-- changing transition between first & second slide -->
        <transition num="3" slicing="vertical" direction="down" shader="flat" />
        <slide>

           <url>http://localhost/quickstart/public/images/gavrilovic/2.jpg</url>
        </slide>		
		<!-- changing transition between second & third slide -->
        <transition num="3" slicing="vertical" direction="down" shader="flat" />
		<slide>
            <url>http://localhost/quickstart/public/images/gavrilovic/3.jpg</url>

        </slide>
        <!-- transitions properties defined in transitions template -->
        <transition num="3" slicing="vertical" direction="down" shader="flat" />
		<slide>
       		<url>http://localhost/quickstart/public/images/gavrilovic/4.jpg</url>

        </slide>
        <transition num="3" slicing="vertical" direction="down" shader="flat" />
        <slide>
       		<url>http://localhost/quickstart/public/images/gavrilovic/5.jpg</url>

        </slide>
        <transition num="3" slicing="vertical" direction="down" shader="flat" />				
	</slides>
</cu3er>

